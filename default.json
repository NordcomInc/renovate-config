{
    "extends": [
        "config:base",
        ":labels(Dependency)",
        ":rebaseStalePrs",
        ":disableRateLimiting",
        ":semanticCommitsDisabled"
    ],
    "gradle": {
        "enabled": true
    },
    "gradle-wrapper": {
        "enabled": true
    },
    "maven": {
        "enabled": true
    },
    "python": {
        "enabled": true
    },
    "bundler": {
        "enabled": true
    },
    "travis": {
        "enabled": false
    },
    "automerge": true,
    "allowCustomCrateRegistries": true,
    "allowScripts": true,
    "exposeAllEnv": true
}
