{
    "extends": [
        "config:base",
        ":labels(Dependency)",
        ":rebaseStalePrs",
        ":disableRateLimiting",
        ":semanticCommitsDisabled",
        ":disableDependencyDashboard",
        ":automergeMinor"
    ],
    "gradle": {
        "enabled": true
    },
    "gradle-wrapper": {
        "enabled": true
    },
    "maven": {
        "enabled": true
    },
    "python": {
        "enabled": true
    },
    "bundler": {
        "enabled": true
    },
    "travis": {
        "enabled": false
    },
    "automerge": true,
    "allowCustomCrateRegistries": true,
    "allowScripts": true,
    "exposeAllEnv": true,
    "onboardingPrTitle": "Configure Renovate",
    "onboardingNoDeps": true,
    "commitMessage": "{{{commitMessagePrefix}}}{{{commitMessageAction}}} {{{commitMessageTopic}}} {{{commitMessageExtra}}}{{{commitMessageSuffix}}}",
    "commitMessagePrefix": "Dependencies: ",
    "commitMessageSuffix": ".",
    "prConcurrentLimit": 2,
    "prFooter": "This PR has been generated on behalf of [Nordcom Group Inc.](https://nordcom.io/) by [Renovate Bot](https://github.com/renovatebot/renovate)."
}
